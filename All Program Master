!! question number 1 !! write a c++ program to show '*' and '+' operator overloading !!

#include <iostream>
#include <string>
using namespace std;

class add {
    public:
        int var;
        add() { }
        void setval(){
        cout<<" enter value :";
        cin>>var;
        }

        add operator+(add &obj) {
            add res;
            res.var= this->var+obj.var;
            return res;
        }
};
class multi {
    public:
        int var;
        multi() { }
        void setval(){
        cout<<" enter value :";
        cin>>var;
        }

        multi operator*(multi &mul) {
            multi multiply;
            multiply.var= this->var*mul.var;
            return multiply;
        }
};

int main() {
    add obj1, obj2;
    obj1.setval();
    obj2.setval();
    add res = obj1+obj2;
    cout <<" addition of object 1 and object 2 is :"<< res.var;
    cout<<endl;
    multi mul1, mul2;
    mul1.setval();
    mul2.setval();
    multi multiply = mul1*mul2;
    cout <<" multipication of object 1 and object 2 is :"<< multiply.var;
    cout<<endl;
}

