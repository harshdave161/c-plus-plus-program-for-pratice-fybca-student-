Question number 12 !! write a program using inline function to calculate square og given number(no.) !!

#include<iostream>
#include<conio.h>
using namespace std;
class square{
public:
    int num;
    void getdata(){
    cout<<" enter number to calculate square of number : ";
    cin>>num;
    }
    inline int dispsquare(){
    int squ;
    squ=num*num;
    return squ;
    }
    void disp(){
    cout<<" square is : "<<dispsquare();
    }
};
int main(void){
square s1;
s1.getdata();
s1.dispsquare();
s1.disp();
}

