#include<iostream>
#include<conio.h>
using namespace std;
// made by harsh dave 2019 ©

    int main() {
    	int num,factorial=1;
    	cout<<" Enter Number To Find Its Factorial:  ";
    	cin>>num;
    	for (int a=1;a<=num;a++) {
    		factorial=factorial*a;
    	}
    	cout<<"Factorial of Given Number is ="<<factorial<<endl;
    	return 0;
    }

