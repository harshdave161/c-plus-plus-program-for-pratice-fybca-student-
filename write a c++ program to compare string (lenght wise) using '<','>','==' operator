#include <iostream>
#include<string.h>
using namespace std;
// made by harsh dave 2019 ©
int main()
{
    string str_1,str_2;
    cout<<" Enter string one : ";
    getline(cin,str_1);
    cout<<endl;
    cout<<" Enter string two : ";
    getline(cin,str_2);

    if(str_1>str_2){
        cout<<" string 1 is greater than string 2 "<<endl;
    }
    else if(str_1==str_2){
        cout<<" string 1 and 2 are equal "<<endl;
    }
    else{
        cout<<" string 2 is greater than string 1 "<<endl;
    }


	return 0;
}


